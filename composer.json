{
  "name": "drupal/commerce_recruitment",
  "type": "drupal-module",
  "description": "Commerce Recruitment",
  "keywords": ["drupal", "web", "ui"],
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Paul Mickan",
      "email": "paul.mickan@key-tec.de"
    },
    {
      "name": "David Stangl",
      "email": "david.stangl@key-tec.de"
    }
  ],
  "require-dev": {
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "~1.4",
    "drupal-composer/drupal-scaffold": "^2.2",
    "drupal/admin_toolbar": "^2.0",
    "drupal/coffee": "~1",
    "drupal/commerce": "^2.16",
    "drupal/commerce_product_bundle": "1.x-dev",
    "drupal/config_devel": "~1",
    "drupal/config_installer": "~1",
    "drupal/console": "~1",
    "drupal/ctools": "^3.2",
    "drupal/devel_entity_updates": "^1.0@RC",
    "drupal/dynamic_entity_reference": "^1.8",
    "drupal/twig_tweak": "^2.4",
    "drush/drush": "~9",
    "openeuropa/drupal-core-require-dev": "~8",
    "openeuropa/task-runner": "~1.0-beta6",
    "phpro/grumphp": "~0.14"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "autoload": {
    "psr-4": {
      "Drupal\\commerce_recruitment_settings\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\Tests\\commerce_recruitment_settings\\": "./tests/src"
    }
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": "./vendor/bin/run drupal:site-setup",
    "post-update-cmd": "./vendor/bin/run drupal:site-setup"
  },
  "extra": {
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "installer-paths": {
      "build/core": ["type:drupal-core"],
      "build/modules/contrib/{$name}": ["type:drupal-module"],
      "build/profiles/contrib/{$name}": ["type:drupal-profile"],
      "build/themes/contrib/{$name}": ["type:drupal-theme"]
    }
  },
  "config": {
    "sort-packages": true
  }
}
