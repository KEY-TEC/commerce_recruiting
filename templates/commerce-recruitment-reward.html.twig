{#
/**
 * @file commerce-recruitment-reward.html.twig
 * Default theme implementation to present reward data.
 *
 * This template is used when viewing reward pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_commerce_recruitment_reward()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('commerce_recruitment_reward') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
